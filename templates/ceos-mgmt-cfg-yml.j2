!
! Base Config for {{ item.key }}
!
daemon TerminAttr
   exec /usr/bin/TerminAttr -ingestgrpcurl=10.112.112.2:9910 -taillogs -ingestauth=key,h0m3k3y -smashexcludes=ale,flexCounter,hardware,kni,pulse,strata -ingestexclude=/Sysdb/cell/1/agent,/Sysdb/cell/2/agent -ingestvrf=management
   no shutdown
!
hostname {{ item.key }}
!
vrf instance management
!
interface Management1
   description OOB Management Interface
   vrf management
   ip address {{ item.value.mgmt_ip }}/{{ item.value.mgmt_mask }}
   no lldp transmit
   no lldp receive
!
ip route vrf management 0.0.0.0/0 {{ item.value.mgmt_gateway }}
!
ip routing vrf management
!
management api http-commands
   no shutdown
   !
   vrf management
      no shutdown