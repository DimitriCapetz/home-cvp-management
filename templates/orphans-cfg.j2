{# Create Orphan Port Configs for Leafs #}
{% for k, v in Ports.items() %}
{%   if k == item.hostname %}
{%     for port, config in v.items() %}
interface {{ port }}
   description {{ config['description'] }}
   switchport trunk allowed vlan {{ config['vlans'] }}
   switchport mode trunk
!
{%     endfor %}
{%   endif %}
{% endfor %}